    <!-- Instructional Tour -->
      <script>
        var tour = new Tour({
          steps: [
          {
            orphan: true,
            //element: "#mapContainer",
            title: "Welcome to Mint!",
            content: "We'd like to give you a quick tour to get started."
            
          },
          {
            element: "#step1",
            title: "Learn about us here!",
            content: "Click on the tab to drop down to more options."
          },
          {
            element: "#step2",
            title: "See our visualizations here!",
            content: "Check out the different data we have displayed. Redirects back to dashboard."
          },
          {
            element: "#mapid",
            title: "Select a region marker to view stats!",
            content: "Click on the map to enable scrolling capabilities, and click again to disable...",
            placement: "top"
          },
          {
            element: "#chartContainer",
            title: " ",
            content: "...And the visualizations will update according to which region you have chosen.",
            placement: "top"
          }]

          // backdrop: true,
          // backdropContainer: 'body',
          // backdropPadding: 0

        });
      
        // Initialize the tour
        tour.init();

        // Start the tour
        tour.start();

        $('#helpButton').click(function(event) {
          tour.restart();
        });
